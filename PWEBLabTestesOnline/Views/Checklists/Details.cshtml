@model PWEBLabTestesOnline.Models.Checklist



<h1>@Html.DisplayFor(model => model.Name)</h1>

<div>
    <hr />
    <h4>Procedures</h4>
    @foreach(var procedure in Model.Procedures)
    {   
        <p>@procedure.ProcedureDescription</p>
    }
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ChecklistId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
